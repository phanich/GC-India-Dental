                                  ticketlist[pi].tags[tgindex].tagname=getKeyByValue(TagMap,element[0].children[0].children[0].children[1].innerHTML);
                                  tgindex++;
                                  }
                                  }
                             //end of tags
                            
                        }
                        }
                         
                      
                     
                    }
