          function normalizeDateTimeExtended(mydate){
 
    var now= new Date(mydate-(3600000*12)),
    ampm= 'AM', 
    h= now.getUTCHours(), 
    m= now.getUTCMinutes(), 
    s= now.getUTCSeconds();
    
  
    var d = now.getDate();  
   var mo = now.getMonth()+1;  
   var y = now.getFullYear(); 
    if(h>= 12){
        if(h>12) h -= 12;
        ampm= 'PM';
    }

    if(m<10) m= '0'+m;
    if(s<10) s= '0'+s;
    return ''+ (mo<=9?'0'+mo:mo) +'/' + (d<=9?'0'+d:d)+'/' + y+ ' ' +addHours(h + ':' + m + ' ' + ampm, -4)+' '+ampm ;
   //return ''+ (m<=9?'0'+m:m) +'/' + (d<=9?'0'+d:d)+'/' + y+' '+mydate.toLocaleString().replace(/:\d+ /, ' ');  
 }
 
 // Assumes timeString is hh:mm am/pm
function addHours(timeString, h) {
  var t = timeString.match(/\d+/g);
  var am = /am$/.test(timeString);
  var d = new Date();
  d.setHours(+t[0] + (am? 0 : 12) + +h, +t[1]);
  return formatTime(d.getHours() + ':' + d.getMinutes());
}
function formatTime(t) {
  function addZ(n) {
    return n<10? '0'+n : ''+n;
  }

  var t = t.split(':');
  

  return addZ(t[0]%12 || t[0]) + ':' + addZ(t[1]) ;
}
 
