    <apex:outputPanel styleClass="popupBackgroundForMorePopUp" layout="block" id="eventMoreOptionsText" style="width:100%; height:100%; position:fixed; top:event.pageY; left: event.pageX" rendered="{!hashTagsBool}"/>  
        <apex:outputPanel styleClass="custPopupForMorePopUp" style="width:80%; position: fixed; padding-bottom:70px;" rendered="{!hashTagsBool}">   
                Display Hash tags Table 
           <script>
                    choosenForPopUp();
                </script>       
          <apex:repeat value="{!ticketmoreTic}" var="tic">     
                
                <apex:variable value="{!0}" var="hashTagVariable"/>
                <table style="width:100%; border=2px; ">    
                    <tr>
                        <td>  Select HashTags  </td>
                        <td>   
                            <apex:selectList value="{!selectedTagsFromOptions}" multiselect="false" size="1" styleClass="chzn-select medium-select select" style="width:80%" id="eventHashTags">
                                 <apex:selectOptions value="{!eventHashTags}"/>
                                 <apex:actionSupport action="{!addTagItems}" event="onchange" /> 
                             </apex:selectList>
                          </td>
                    </tr>  
                    
                    <tr>
                        <td>  HashTag </td>
                        <td>  Action    </td>
                    </tr>
                    <apex:repeat value="{!tic.hashTagsForPool}" var="hashtag">
                        <tr>
                            <td> <apex:outputText value="{! hashtag.Tag_Text__c }" />  </td>
                            <td> <apex:commandLink value="Delete" action="{!deleteHashTags}"> 
                                    <apex:param value="{!hashTagVariable}" name="toDeletehashTags"/>
                                 </apex:commandLink>
                                 
                                 <apex:variable value="{!(hashTagVariable+1)}" var="hashTagVariable"/>
                            </td>
                        </tr>
                    </apex:repeat>  
                    <apex:outputText rendered="{!IF(tic.hashTagsForPool.size = 0, TRUE, FALSE)}">
                        <tr> <td colspan="2">
                                    No Tags Found, Please add First tag.
                             </td>
                        </tr>
                   </apex:outputText> 
                    
                     <tr> <td  colspan="2">
                                 &nbsp;  </td>
                        </tr>
                        
                        <tr> <td  >
                                 <apex:inputText value="{!addNewTagItems}"/> </td>
                             <td> 
                                 <apex:commandLink value="Update" action="{!addTagItems}"  reRender="eventHashTagsOptions"/>   </td>
                                  
                        </tr>
               </table>     
                 <apex:commandButton value="SAVE" action="{!saveTagsToTickets}" rerender="eventHashTagsOptions, ticketMoreOptions, hashtagsForRender"/>           
         </apex:repeat>        
        
        </apex:outputPanel> 
 </apex:form>            
