 <apex:form id="eventHashTagstoPackageLineItems"> 
    <apex:outputPanel styleClass="popupBackgroundForMorePopUp" layout="block" id="packageTagsBackground" style="width:100%; height:100%; position:fixed; top:event.pageY; left: event.pageX" rendered="{!pakHashTagsBool}"/>  
        <apex:outputPanel styleClass="custPopupForMorePopUp" style="width:80%; padding-bottom:70px;" rendered="{!pakHashTagsBool}" id="hashTagstoPackageLineItemsPannel">   
                Display Hash tags Table  {!toaddTagsToPackageLineItems}
           <script>
                choosenForPopUp();
            </script>      
          <apex:variable value="{!0}" var="packageHashTag"/>
           
                <apex:variable value="{!0}" var="hashTagVariable"/>
                
                <table style="width:100%; border=2px; ">    
                    <tr>
                        <td>  Select HashTags  </td>
                        <td>   
                             <apex:selectList value="{!addNewTagForPackage}" multiselect="false" size="1" styleClass="chzn-select medium-select select" style="width:80%" id="eventHashTags">
                                 <apex:selectOptions value="{!eventHashTags}"/>
                                 <apex:actionSupport action="{!addTagToPackageLineItem}" event="onchange" /> 
                             </apex:selectList> 
                          </td>
                    </tr>  
                    
                    <tr>
                        <td>  HashTag </td>
                        <td>  Action    </td>
                    </tr>
                    <apex:repeat value="{!newPackageItems.packageItemPoolTags}" var="pakLineItems"> 
                        
                        <apex:outputText rendered="{!packageHashTag = toaddTagsToPackageLineItems}">     
                              <apex:repeat value="{!pakLineItems.itemPoolStrings}" var="hashtag">
                                <tr>
                                    <td> <apex:outputText value="{! hashtag }" />  </td>
                                    <td> <apex:commandLink value="Delete" action="{!deleteTagFromPackageLineItem}" reRender="eventHashTagstoPackageLineItems"> 
                                            <apex:param value="{!hashTagVariable}" name="toDeletePackageHashTags"/>
                                         </apex:commandLink>
                                         
                                         <apex:variable value="{!(hashTagVariable+1)}" var="hashTagVariable"/>
                                    </td>
                                </tr>
                                </apex:repeat>   
                            
                                <apex:outputText rendered="{!IF(pakLineItems.itemPoolStrings.size = 0, TRUE, FALSE)}">
                                    <tr> <td colspan="2">
                                                No Tags Found, Please add First tag.
                                         </td>
                                    </tr>
                               </apex:outputText> 
                        </apex:outputText>  
                        
                        <apex:variable value="{!packageHashTag+1}" var="packageHashTag"/>
                        
                    </apex:repeat>
                     
                    
                    
                     <tr> <td colspan="2">
                                 &nbsp;  </td>
                        </tr>
                        
                        <tr> <td>
                                 <apex:inputText value="{!addNewTagForPackage}"/> 
                             </td>
                             <td> 
                                 <apex:commandLink value="Update" action="{!addTagToPackageLineItem}"  reRender="eventHashTagstoPackageLineItems"/>   <!----  reRender="hashTagstoPackageLineItemsPannel"  ---->
                             </td>
                        </tr>
               </table>     
                 <apex:commandButton value="SAVE" action="{!saveTagsToPackageLineItems}" rerender="eventHashTagstoPackageLineItems, newPackagesLine"/>           
                 
            -------------->
        </apex:outputPanel> 
        
      <apex:outputPanel id="paymentOptionsAfterSave">
           <apex:repeat value="{!finallToSaveGateWays}" var="f">
               <apex:outputText value="{!f.Name}"/>
           </apex:repeat> 
      </apex:outputPanel>
   
      
     <apex:actionFunction action="{!payGatewayFromJs}" name="gateWayfromJs" reRender="paymentOptionsAfterSave">
